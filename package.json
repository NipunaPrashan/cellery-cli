{
  "name": "cellery",
  "description": "Design, develop, build, run, manage your cells anywhere",
  "version": "1.0.0",
  "author": "NipunaPrashan @NipunaPrashan",
  "bin": {
    "cellery": "./bin/run"
  },
  "bugs": "https://github.com/NipunaPrashan/cellery/issues",
  "dependencies": {
    "@oclif/command": "^1.5.4",
    "@oclif/config": "^1.8.8",
    "@oclif/plugin-help": "^2.1.3",
    "@types/supports-color": "^5.3.0",
    "cli": "^1.0.1",
    "cli-frames": "^2.2.8",
    "cli-ux": "^4.9.1",
    "colors": "^1.3.2",
    "figures": "^2.0.0",
    "listr": "^0.14.2",
    "makensis": "^0.16.3",
    "ora": "^3.0.0",
    "prex": "^0.4.3",
    "tslib": "^1.9.3"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1.19.4",
    "@oclif/test": "^1.2.2",
    "@oclif/tslint": "^1.1.2",
    "@types/chai": "^4.1.7",
    "@types/mocha": "^5.2.5",
    "@types/node": "^10.12.0",
    "chai": "^4.2.0",
    "globby": "^8.0.1",
    "mocha": "^5.2.0",
    "nyc": "^12.0.2",
    "ts-node": "^7.0.1",
    "tslint": "^5.11.0",
    "typescript": "^3.1.3"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/NipunaPrashan/cellery",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "cellery",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "NipunaPrashan/cellery",
  "scripts": {
    "postpack": "rm -f oclif.manifest.json npm-shrinkwrap.json",
    "posttest": "tsc -p test --noEmit && tslint -p test -t stylish",
    "prepack": "rm -rf lib && tsc && oclif-dev manifest && oclif-dev readme && npm shrinkwrap",
    "prepare": "rm -rf lib && tsc",
    "test": "nyc mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif-dev readme && git add README.md"
  },
  "types": "lib/index.d.ts"
}
